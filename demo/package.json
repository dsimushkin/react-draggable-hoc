{
  "name": "react-draggable-hoc-demo",
  "version": "0.0.10",
  "private": true,
  "main": "src/index.ts",
  "homepage": "http://dsimushkin.github.io/react-draggable-hoc",
  "dependencies": {
    "raf": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-draggable-hoc": "latest",
    "react-router-dom": "latest",
    "react-scripts": "latest"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "upgrade:manual": "rm -rf node_modules yarn.lock && yarn install",
    "commit:lock": "git commit yarn.lock -m \"Upgrade yarn lock\"",
    "rmlink": "yarn unlink react-draggable-hoc",
    "predeploy": "yarn rmlink && yarn upgrade:manual && yarn build",
    "deploy": "gh-pages -d build",
    "postdeploy": "yarn commit:lock"
  },
  "devDependencies": {
    "@types/jest": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-router-dom": "latest",
    "gh-pages": "latest",
    "typescript": "latest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
