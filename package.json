{
  "name": "react-draggable-hoc",
  "version": "1.1.0",
  "description": "drag by ref using hook",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "prebuild": "rm -rf dist && rm -rf styles",
    "watch": "rollup -cw",
    "prewatch": "yarn prebuild",
    "prepare": "yarn build",
    "postpublish": "(cd demo && yarn deploy) && (git push && git push --tags)"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "rollup": "^1.14.4",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "directories": {
    "dist": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fellaslim/react-draggable-hoc.git"
  },
  "keywords": [
    "draggable",
    "react",
    "hook",
    "dnd",
    "dragndrop",
    "drag",
    "drop"
  ],
  "author": "Simushkin Dmitry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fellaslim/react-draggable-hoc/issues"
  },
  "homepage": "https://github.com/fellaslim/react-draggable-hoc#readme"
}
